# Monit logrotate configuration

# Environment: <%= env_name %>
# Revision: <%= revision %>
# Created at <%= now %>

<%= log_file_path %> {
  daily
  missingok
  rotate 14
  compress
  dateext
  notifempty
  sharedscripts
  postrotate
    [ ! -f <%= pid_file_path %> ] || kill -HUP $(cat <%= pid_file_path %>) 2>/dev/null || true
  endscript
}
